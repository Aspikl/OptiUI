						<label y="-1" fitx="true" fitxpadding="0.7h" font="dyn_10" textalign="center" textvalign="center" color=".1 .1 .1 1" shadow="true" shadowcolor=".8 .8 .8 1"
							onshow="SetText(GetKeybindButton('game', '{command}', ''));"

						/>
					</panel>
				</panel>
